s = spec.getSpec(4565636637219987456)
s_pd = spec.getSpec(4565636637219987456, fmt='pandas')
s_sp = spec.getSpec(4565636637219987456, fmt='Spectrum1D')

print(str(type(s)))
print(str(type(s['sky'])))
print(str(type(s_pd)))
print(str(type(s_sp)))

print (len(s_pd['loglam']))
print (len(s_sp.flux))
print (len(s_sp.wavelength))
print (s_sp.meta['sky'])
